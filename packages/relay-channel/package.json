{
  "name": "clawrelay",
  "version": "0.3.3",
  "description": "Channel relay plugin for OpenClaw â€” receives messages from an always-on relay proxy",
  "main": "src/index.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/kylemclaren/clawrelay.git",
    "directory": "packages/relay-channel"
  },
  "openclaw": {
    "extensions": [
      "./src/index.ts"
    ],
    "channel": {
      "id": "relay",
      "label": "Channel Relay",
      "selectionLabel": "Relay (Discord/Telegram via proxy)",
      "docsPath": "/channels/relay",
      "blurb": "Wake-on-message proxy for Discord/Telegram via always-on relay",
      "aliases": ["relay"]
    },
    "install": {
      "npmSpec": "clawrelay",
      "localPath": "extensions/relay-channel",
      "defaultChoice": "npm"
    }
  },
  "keywords": [
    "openclaw",
    "openclaw-plugin",
    "relay",
    "channel",
    "discord",
    "telegram",
    "proxy"
  ],
  "author": "Snowy Road",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.3.0"
  },
  "scripts": {
    "test": "echo \"No tests yet\""
  }
}
